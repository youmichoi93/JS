//메세지를 던져서 메세지를 출력.
function print(msg){
  console.log("print function in");
  console.log(msg);
  console.log("prin function out");
}


//인수를 더한 값을 결과로 출력.
function sum(arg1,arg2){
  var result = arg1 + arg2;
  return result;
}
